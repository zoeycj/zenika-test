ord) => record.gender.indexOf(value) === 0,\n    },\n    {\n      title: 'Action',\n      key: '',\n      align: 'center',\n      render(employee) {\n        return (\n          <Row justify=\"space-around\">\n            <Col span={12} justify=\"center\">\n              <EditOutlined onClick={() => onEdit(employee)} />\n            </Col>\n            <Col span={12} justify=\"center\">\n              <DeleteOutlined onClick={() => onDelete(employee)} />\n            </Col>\n          </Row>\n        )\n      },\n    },\n  ]\n\n  return (\n    <>\n      <Table\n        bordered\n        rowKey=\"_id\"\n        columns={columns}\n        dataSource={props.employees}\n        pagination={{ defaultPageSize: PAGE_SIZE }}\n      />\n      <Modal\n        visible={visible}\n        onOk={handleOk}\n        onCancel={handleCancel}\n        okText=\"confirm\"\n        cancelText=\"Cancel\"\n      >\n        <p>Are you sure you want to delete?</p>\n      </Modal>\n    </>\n  )\n}\nexport default ListTable\n","/Users/kevin/work/my code02/zenika-test/src/state/ducks/employee/index.js",[],"/Users/kevin/work/my code02/zenika-test/src/state/ducks/employee/types.js",[],"/Users/kevin/work/my code02/zenika-test/src/state/ducks/employee/reducers.js",[],"/Users/kevin/work/my code02/zenika-test/src/state/api/index.js",[],{"ruleId":"80","severity":1,"message":"81","line":38,"column":5,"nodeType":"82","messageId":"83","endLine":38,"endColumn":16},{"ruleId":"80","severity":1,"message":"81","line":36,"column":5,"nodeType":"82","messageId":"83","endLine":36,"endColumn":16},{"ruleId":"80","severity":1,"message":"81","line":52,"column":3,"nodeType":"82","messageId":"83","endLine":52,"endColumn":14},"no-console","Unexpected console statement.","MemberExpression","unexpected"]